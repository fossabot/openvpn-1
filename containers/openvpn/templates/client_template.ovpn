client
proto udp
remote $SERVER_REMOTE_ADDR
port 1194
dev tun
nobind

key-direction 1

<ca>
$(cat $KEY_DIR/ca.crt)
</ca>

<cert>
$(cat $KEY_DIR/client1.crt)
</cert>

<key>
$(cat $KEY_DIR/client1.key)
</key>

<tls-auth>
-----BEGIN OpenVPN Static key V1-----
# insert ta.key
-----END OpenVPN Static key V1-----
</tls-auth>